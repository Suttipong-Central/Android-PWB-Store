<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="typeFaceAsset" format="string"/>

    <declare-styleable name="PowerBuyTextView">
        <attr name="typeFaceAsset"/>
    </declare-styleable>

    <declare-styleable name="PowerBuyEditText">
        <attr name="typeFaceAsset"/>
    </declare-styleable>

    <declare-styleable name="PowerBuyIncreaseOrDecreaseView">
        <attr name="qty"/>
    </declare-styleable>
    <declare-styleable name="PowerBuyEditTextBorder">
        <attr name="header" format="string"/>
        <attr name="required" format="boolean"/>
        <attr name="pwb_icon" format="reference"/>
    </declare-styleable>
    <declare-styleable name="PowerBuyAutoCompleteTextStroke">
        <attr name="act_header" format="string"/>
        <attr name="act_required" format="boolean"/>
        <attr name="hide_header" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="PowerBuyBackButton">
        <attr name="disable" format="boolean"/>
        <attr name="hide_icon" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="PowerBuyIconButton">
        <attr name="btn_disable" format="boolean"/>
        <attr name="icon_invisible" format="boolean"/>
        <attr name="btn_default" format="boolean"/>
        <attr name="text" format="string"/>
    </declare-styleable>
</resources>